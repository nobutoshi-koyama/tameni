<div class="content5">
  
  <% @tweet = Tweet.new unless @tweet %>
  <%= form_for(@tweet) do |f| %>
  
    
      <div class="row">
        <!--<div class="input-field col s12">-->
          
          <%= f.label :menu %>
          <%= f.text_field :menu, placeholder:"品目"%>
          
          <%= f.label :quantity %>
          <%= f.text_field :quantity %>
          
          <%= f.label :start_year %>
          <%= f.text_field :start_year %>
          
          <%= f.label :start_month %>
          <%= f.text_field :start_month %>
          
          <%= f.label :start_day %>
          <%= f.text_field :start_day %>
          
          <%= f.label :end_year %>
          <%= f.text_field :end_year %>
          
          <%= f.label :end_month %>
          <%= f.text_field :end_month %>
          
          <%= f.label :end_day %>
          <%= f.text_field :end_day %>
          
          
          
          <div class="form-group">
          <%= f.submit "送信", class: "btn btn-primary withripple" %>
          </div>
          
          <% end %>
        </div>
      <!--</div>-->
     
  </div>
  
  
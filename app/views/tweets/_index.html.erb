<div class="user_all">
  
       <% user.tweets.each do |tweet| %>
       
       <tbody>
       <td><%= tweet.menu %></td>
        <td><%= tweet.text %></td>
        <td><%= tweet.quantity %></td>
        <td><%= tweet.dayoftweet.year %>年<%= tweet.dayoftweet.day %>月<%= tweet.start_day %></td>
        <td><%= tweet.end_month %>月<%= tweet.end_day %>日</td>
        
         
          
           
           </tbody>
            
               
       
<% end %>



   
   </div>
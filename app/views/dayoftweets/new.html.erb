<div class="content5">
  
  <% @dayoftweet = Dayoftweet.new unless @dayoftweet %>
  <%= form_for(@dayoftweet) do |f| %>
  
    
      <div class="row">
        <!--<div class="input-field col s12">-->
          
          
          <%= f.label :year %>
          <%= f.select :year, options_for_select(2019..2024), {include_blank: "選択してください"}, {class: "browser-default"} %>
          
          <%= f.label :month %>
          <%= f.select :day, options_for_select(1..12), {include_blank: "選択してください"}, {class: "browser-default"} %>
          
          
          
         
          
          
          
          <div class="form-group">
          <%= f.submit "送信", class: "btn btn-primary withripple" %>
          </div>
          
          <% end %>
        </div>
      <!--</div>-->
     
  </div>
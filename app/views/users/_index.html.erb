<div class="row">
  
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              
              <span class="card-title"><%= user.name %>さんの出荷報告</span>
              
              <table>
               <thead>
                <tr>
                  <th>品目</th>
                  <th>数量(pc)/日</th>
                  <th>出荷開始日</th>
                  <th>出荷終了日</th>
                </tr>
               </thead>

       <% @tweets.each do |tweet| %>
              
              
   
  
  <tbody>
    <tr>
      <td><%= tweet.menu %></td>
      <td><%= tweet.quantity %></td>
      <td><%= tweet.start_year %>年<%= tweet.start_month %>月<%= tweet.start_day %></td>
      <td><%= tweet.end_year %>年<%= tweet.end_month %>月<%= tweet.end_day %></td>
     
    </tr>
  </tbody>
  
  
<% end %>
</table>
        </div>
        </div>
        
          
        </div>
</div>
      
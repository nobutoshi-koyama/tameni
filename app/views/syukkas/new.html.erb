<div class="content7">
  
  <% @syukka = Syukka.new unless @syukka %>
  <%= form_for(@syukka) do |f| %>
  
    
      <div class="row">
        <!--<div class="input-field col s12">-->
          
          <%= f.label :年 %>
          <%= f.text_field :year %>
          
          <%= f.label :月 %>
          <%= f.text_field :month %>
          
          <%= f.label :日 %>
          <%= f.text_field :day %>
          
          <%= f.label :生産者 %>
          <%= f.text_field :farmer %>
          
          
          <%= f.label :品目 %>
          <%= f.collection_select :menu, Tweet.all, :menu, :menu ,{prompt: ""}, {class: "browser-default"} %>
          
        　 <%= f.label :品種 %>
          <%= f.text_field :detail %>
          
          <%= f.label :quantity %>
          <%= f.text_field :quantity %>
          
          
          
         
          
          
          
          <div class="form-group">
          <%= f.submit "送信", class: "btn btn-primary withripple" %>
          </div>
          
          <% end %>
        </div>
      <!--</div>-->
     
  </div>
  
  
<div class="content6">

     <% @memo = Memo.new unless @memo %>
    
     
      
      <%= form_for(@memo) do |f| %>
      <div class="form-group file">
        <%= f.file_field :image, class: "form-control floating-label", placeholder: "画像アップロード" %>
      </div>
      
      <%= f.label :title %>
      <%= f.text_field :title%>
      
      <%= f.label :text %>
      <%= f.text_field :text %>
      
      
      
      <div class="form-group">
        <%= f.submit "送信", class: "btn btn-primary withripple" %>
      </div>
      <% end %>
</div>